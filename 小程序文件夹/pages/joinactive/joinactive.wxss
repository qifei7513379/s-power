
.joinlist-item{height:100rpx;margin: 0 30rpx;border-bottom: 1px solid #efeff4;display: flex}
.joinlist-item label{display: block;height: 100rpx;line-height: 100rpx;vertical-align: top;margin-right: 20rpx;font-size: 30rpx;color: #3b3b3b;text-align: justify;flex: 0 0 22%}
.joinlist-item input{display: block;height: 100rpx;line-height: 100rpx;vertical-align: top;font-size: 24rpx;flex: 1}
.joinlist-item label text{color: #ffba00;font-size: 24rpx;margin-left: 10rpx}
.count view{display: block;flex: 0 0 70rpx;height: 70rpx;background: #efeff4;text-align: center;line-height: 70rpx}
.count text{display: block;flex: 1;border-top: 1rpx solid #e1e1e1;border-bottom: 1px solid #e1e1e1;height: 66rpx;text-align: center;line-height: 66rpx;font-size: 30rpx}
.zhanwei{flex: 1}
.count{flex: 0 0 30%;display: flex;margin-top: 15rpx}
.joinactive-foot{width: 100%;height: 88rpx;border-top: 1px solid #e1e1e1;position: fixed;bottom: 0;left: 0;display: flex}
.joinactive-foot .totle{flex: 1;color: #ff4e00;font-size: 24rpx;line-height: 88rpx;margin-left: 10rpx}
.joinactive-foot .totle text{font-size: 30rpx}
.joinactive-foot .tel{flex: 0 0 110rpx;font-size: 20rpx;color: #7c7c7c;border-left: 1px solid #e1e1e1;border-right: 1px solid #e1e1e1;text-align: center;line-height: 120rpx;background: url(http://yutingdq.gotoip4.com/xpx/tel.png) no-repeat;background-size: 28rpx 31rpx;background-position: 50% 20%}
.joinactive-foot .yuding{flex: 0 0 263rpx;background: #f89e00;color: white;font-size: 30rpx;line-height: 88rpx;text-align: center}
.info{font-size: 24rpx;color: #999;margin: 30rpx;}