page{background: #f5f5f5}
.pay-foot{width: 100%;height: 90rpx;border-top: 1px solid #e1e1e1;position: fixed;bottom: 0;left: 0;display: flex;line-height: 90rpx;background: white}
.pay-foot .clectall{flex:  0 0 120rpx;font-size: 24rpx;color: #7c7c7c;margin: 0 80rpx 0 30rpx}
.pay-foot .totle{flex: 1;font-size: 24rpx;color: #7c7c7c}
.pay-foot .totle text{color: #ec5e2a;font-size: 30rpx}
.pay-foot .jiesuan{height: 90rpx;background: #f89e00;color: white;text-align: center;line-height: 90rpx;flex: 0 0 240rpx;font-size: 30rpx}
.pay-list{padding: 0 30rpx;background: white;margin: 30rpx 0;height: 180rpx;position: relative}
.pay-list image{width: 180rpx;height: 180rpx;display: inline-block;vertical-align: top}
.pricewrapper{display: inline-block;width: 420rpx;vertical-align: top;margin-left: 20rpx;}
.pricewrapper view:nth-child(1){font-size: 30rpx;color: #2c2c2c;line-height: 50rpx;height: 100rpx;display:-webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;overflow: hidden;}
.pricewrapper view:nth-child(2){font-size: 30rpx;color: #ec5e2a;margin-top: 30rpx;}
.pay-list checkbox-group{display: inline-block;vertical-align: top;margin-top: 65rpx;}
.count view{display: block;flex: 0 0 70rpx;height: 70rpx;background: #efeff4;text-align: center;line-height: 70rpx}
.count text{display: block;flex: 1;border-top: 1rpx solid #e1e1e1;border-bottom: 1px solid #e1e1e1;height: 66rpx;text-align: center;line-height: 66rpx;font-size: 30rpx}
.count{width:230rpx;display: flex;margin-top: 15rpx;position: absolute;bottom: 10rpx;right: 10rpx}
.pay-foot-zhanwei{height: 90rpx}