/*活动列表  */
.index_active image{width: 100%;height: 425rpx;display: block}
.index_active{position: relative}
.index_active .price{position: absolute;color: white;font-size: 24rpx;left: 40rpx;top: 250rpx;width: 120rpx;height: 56rpx;text-align: center;line-height: 46rpx;background: url(http://yutingdq.gotoip4.com/xpx/qipao1.png) no-repeat;background-size: 120rpx 56rpx}
.index_active .title{position: absolute;color: white;font-size: 28rpx;left: 40rpx;top: 320rpx;width: 87%;overflow: hidden;text-overflow:ellipsis;white-space: nowrap}
.index_active .info{position: absolute;color: white;font-size: 24rpx;left: 40rpx;top: 370rpx;}
.index_active .zhegai{background: linear-gradient(-180deg, rgba(0, 0, 0,0), rgba(0, 0, 0,.7));position: absolute;top: 0;left: 0;width: 100%;height: 425rpx}
.index_active .info text{margin-right: 10rpx}
